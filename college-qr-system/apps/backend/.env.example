# Backend environment variables
# Copy this file to .env and fill values for local development

# Database URL used by Prisma (replace with your connection string)
DATABASE_URL=postgresql://user:password@localhost:5432/college_db

# Resend API key for sending OTP emails
RESEND_API_KEY=re_your_actual_api_key_here

# JWT secret
JWT_SECRET=change_this_to_a_long_random_secret

# Server port
PORT=4000
